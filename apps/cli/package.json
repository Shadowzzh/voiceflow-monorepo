{
  "name": "@voiceflow/cli",
  "type": "module",
  "version": "0.0.0",
  "description": "tsup build package template",
  "author": "ziheng <shadow1746556951@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shadowzzh/voiceflow-monorepo.git",
    "directory": "apps/cli"
  },
  "bugs": {
    "url": "https://github.com/Shadowzzh/voiceflow-monorepo/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "exec": "node dist/index.cjs",
    "start": "tsx src/index.ts",
    "dev": "unbuild --stub",
    "build:watch": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "publishConfig": {},
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "enquirer": "^2.4.1",
    "ora": "^8.2.0",
    "undici": "^7.10.0"
  }
}
